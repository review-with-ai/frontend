    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.2",
    "framer-motion": "^11.2.10",
    "ip-address": "^9.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-router-dom": "^6.23.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@storybook/addon-essentials": "^8.1.5",
